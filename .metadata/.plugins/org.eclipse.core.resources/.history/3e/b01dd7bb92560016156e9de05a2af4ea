package homework1;

public class Action {
	
	public static void main(String args[]){
		
		FirstHomeWork YourFirstHomeWork = new FirstHomeWork();
	YourFirstHomeWork.assignment();
	YourFirstHomeWork.todo();
	YourFirstHomeWork.GPA=50;
	System.out.println("is your first assignment"+YourFirstHomeWork.GPA +"percent of your grade");
	YourFirstHomeWork.complete();
	System.out.print("this homework is "+ YourFirstHomeWork.finalgrade(4,6)+ "different assignments");
	YourFirstHomeWork.HWG();
	System.out.println(YourFirstHomeWork.HWG());
	}
	
	
}
