package bank;

public class ConcreteClass extends AccountBalance {
	
private String name;
private String ssn;
private int accountnumber;
private int deposit;



public ConcreteClass(String name, String ssn,int accountnumber){
this.name=name;
this.ssn=ssn;
this.accountnumber=accountnumber; 
}

public ConcreteClass(String name, String ssn, int accountnumber,int deposit){
}
// default constructor
public ConcreteClass(){}
//
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getSsn() {
	return ssn;
}
public void setSsn(String ssn) {
	this.ssn = ssn;
}
public int getAccountnumber() {
	return accountnumber;
}
public void setAccountnumber(int accountnumber) {
	this.accountnumber = accountnumber;
}
public int getDeposit() {
	return deposit;
}
public void setDeposit(int deposit) {
	this.deposit = deposit;
}

}





