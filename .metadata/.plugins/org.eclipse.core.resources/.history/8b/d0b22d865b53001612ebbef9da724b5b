package forloop;

import java.util.Scanner;

public class loops {

	public static void main(String[] args) {
		Scanner scan= new Scanner(System.in);
		String j="are you ok?";
		System.out.println(j);
		String h=scan.nextLine();
		boolean f=false;
	
		if(h.equals("yes")){
				f=true;
				System.out.println("thank you");
			}
			else{ System.out.println(h);
			}
	 scan.nextLine();
	}	
}		

